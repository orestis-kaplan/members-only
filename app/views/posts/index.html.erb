<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% if logged_in? %>
      <h1>Your Posts</h1>
      <%= render @posts %>
    <% else %>
      <h1>All Posts</h1>
      <%= render partial: 'unauthenticated', collection: @posts, as: :post %>
    <% end %>
    <%= link_to 'Create Post',new_post_path %><br>
  </div>
</div>
